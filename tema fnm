  -o-object-fit: capa;
     ajuste ao objeto: capa;
  -o-posição do objeto: centro;
     posição do objeto: centro;
  font-family: "ajuste ao objeto: capa; posição do objeto: centro";
  /* Polyfill do IE11 */
  transformar: escala(1,01);
  /* O 1.01 é necessário para evitar alguns problemas de arredondamento no Chrome */
  transição: opacidade 0,2s facilidade de entrada, transformação 0,95s cúbico-bezier (0,25, 0,46, 0,45, 0,94);
}

