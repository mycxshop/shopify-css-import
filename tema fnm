/**
 * ------------------------------------------------- -------------------
 * LISTA DE BLOCOS
 *
 * Este componente é usado por vários elementos no celular para permitir itens
 * torna-se rolável em celulares e tablets pequenos
 * ------------------------------------------------- -------------------
 */

.lista de bloqueios {
  exibição: flexível;
  flex-flow: coluna agorarap;
  margem: 0 -7px -20px -7px;
}

.lista de blocos--no-flush.lista de blocos--no-flush {
  margem inferior: 0; /* A classe double é para aumentar a especificidade e evitar configurá-la em cada ponto de interrupção */
}

.lista de blocos__item {
  exibição: flexível; /* Permite esticar o conteúdo se diferentes itens de bloco tiverem alturas diferentes */
}

/* Infelizmente o IE11 não gosta de itens flexíveis aninhados, então para o IE11, revertemos para display: block */
@media screen e (-ms-high-contrast: ativo), (-ms-high-contrast: nenhum) {
  .lista de blocos__item {
    exibição: bloco;
  }
}

