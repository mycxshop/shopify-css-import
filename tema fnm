/** Variação de sobreposição **/
.slideshow__slide--overlay::before {
  posição: absoluta;
  contente: "";
  altura: 100%;
  largura: 100%;
  esquerda: 0;
  superior: 0;
  plano de fundo: gradiente linear (para baixo, rgba (0, 0, 0, 0), rgba (0, 0, 0, 0,3));
  índice z: 1;
}

/** Preserva a variação da proporção

 NOTA DE IMPLEMENTAÇÃO: Estou usando duas maneiras diferentes de centralizar o conteúdo. Se a apresentação de slides tiver altura fixa, o tema usa um flexbox
                      abordagem baseada, enquanto se a apresentação de slides não for fixa (ou seja, respeitando a proporção da imagem), eu uso uma abordagem baseada em transformação
                      abordagem. Eu poderia ter simplificado e usado a transformação baseada em qualquer lugar (o que teria removido muito código),
                      mas foi comprovado que a transformação baseada reduz às vezes a qualidade do texto e o torna um pouco embaçado. Isso é por que
                      existe outra maneira mais otimizada de posicionar quando for possível
 **/
.slideshow--preserve-ratio {
  altura: automático! Importante;
}

.slideshow--preserve-ratio .slideshow__slide {
  altura: automático;
}

.slideshow--preserve-ratio .slideshow__slide-inner {
  posição: relativa;
  exibição: bloco;
  largura: automático;
  altura: automático;
}

.slideshow--preserve-ratio .slideshow__content-wrapper {
  posição: absoluta;
  superior: 50%;
  esquerda: 50%;
  transformar: traduzir(-50%, -50%);
}

@media screen e (largura mínima: 641px) {
  .apresentação de slides:não(.apresentação de slides--edge2edge) {
    margem: var(--desktop-container-gutter) 0;
  }

  .apresentação de slides: não (.apresentação de slides--edge2edge) .flickity-viewport {
    raio da borda: 10px;
  }

  .slideshow__image-placeholder {
    altura: automático;
    largura: 100%;
  }

  /* Posicionamento para carrossel de altura fixa */
  .slideshow__slide-inner--canto superior esquerdo,
  .slideshow__slide-inner--centro superior,
  .slideshow__slide-inner--canto superior direito {
    justificar-conteúdo: flex-start;
  }

  .slideshow__slide-inner - canto inferior esquerdo,
  .slideshow__slide-inner--centro inferior,
  .slideshow__slide-inner--canto inferior direito {
    justificar conteúdo: flex-end;
  }

  .slideshow__slide-inner--bottom-left .slideshow__content-wrapper,
  .slideshow__slide-inner--bottom-center .slideshow__content-wrapper,
  .slideshow__slide-inner--canto inferior direito .slideshow__content-wrapper {
    preenchimento inferior: 75px;
    preenchimento superior: 25px;
  }

  .slideshow__slide-inner--canto superior esquerdo,
  .slideshow__slide-inner--meio-esquerdo,
  .slideshow__slide-inner--canto inferior esquerdo {
    alinhamento de texto: esquerda;
  }

  .slideshow__slide-inner--canto superior esquerdo .slideshow__content,
  .slideshow__slide-inner--meio-esquerdo .slideshow__content,
  .slideshow__slide-inner--canto inferior esquerdo .slideshow__content {
    margem esquerda: 0;
  }

  .slideshow__slide-inner--canto superior direito,
  .slideshow__slide-inner - meio-direita,
  .slideshow__slide-inner--canto inferior direito {
    alinhamento de texto: certo;
  }
  .slideshow__slide-inner--canto superior direito .slideshow__content,
  .slideshow__slide-inner--meio-direita .slideshow__content,
  .slideshow__slide-inner--canto inferior direito .slideshow__content {
    margem direita: 0;
  }

  /* Posicionamento para carrossel de altura não fixa */
  .slideshow--preserve-ratio .slideshow__slide-inner--superior esquerdo .slideshow__content-wrapper,
  .slideshow--preserve-ratio .slideshow__slide-inner--top-center .slideshow__content-wrapper,
  .slideshow--preserve-ratio .slideshow__slide-inner--canto superior direito .slideshow__content-wrapper {
    superior: 0;
    transformar: traduzirX(-50%);
  }

  .slideshow--preserve-ratio .slideshow__slide-inner--bottom-left .slideshow__content-wrapper,
  .slideshow--preserve-ratio .slideshow__slide-inner--bottom-center .slideshow__content-wrapper,
  .slideshow--preserve-ratio .slideshow__slide-inner--bottom-right .slideshow__content-wrapper {
    topo: automático;
    inferior: 0;
    transformar: traduzirX(-50%);
  }
}

