/**
 * ------------------------------------------------- ---------------------------
 * DICA
 * ------------------------------------------------- ---------------------------
 */

@media (-moz-touch-enabled: 0), (hover: hover) {
  [dica de dados] {
    posição: relativa;
  }

  [dica de ferramenta de dados]::antes {
    posição: absoluta;
    conteúdo: attr(dica de ferramenta de dados);
    inferior: calc(100% + 3px);
    esquerda: 50%;
    preenchimento: 2px 11px;
    espaço em branco: nowrap;
    plano de fundo: var(--cor de destaque);
    cor: var(--fundo secundário);
    tamanho da fonte: calc(var(--base-text-font-size) - (var(--default-text-font-size) - 13px));
    eventos de ponteiro: nenhum;
    visibilidade: oculta;
    opacidade: 0;
    raio da borda: 3px;
    sombra da caixa: 0 1px rgba (0, 0, 0, 0,065);
    transição: visibilidade 0,2s de atenuação, opacidade 0,2s de atenuação;
    índice z: 1;
    transformar: traduzirX(-50%);
  }

  [dica de ferramenta de dados]::depois {
    posição: absoluta;
    contente: "";
    esquerda: calc(50% - 7px);
    inferior: calc(100% - 2px);
    largura: 0;
    altura: 0;
    largura da borda: 6px;
    estilo de borda: sólido;
    cor da borda: transparente transparente var(--accent-color) var(--accent-color);
    visibilidade: oculta;
    índice z: 1;
    opacidade: 0;
    transição: visibilidade 0,2s de atenuação, opacidade 0,2s de atenuação;
    transformar: girar (-45 graus);
    sombra da caixa: -1px 1px 1px 0 rgba(0, 0, 0, 0,1);
  }

  [dica de ferramenta de dados]:hover::before, [dica de ferramenta de dados]:hover::depois {
    opacidade: 1;
